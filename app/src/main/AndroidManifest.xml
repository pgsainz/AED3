<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.asistentedeahorro">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Detalle"
            android:label="@string/title_activity_detalle"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".NvoEgreso"
            android:label="@string/title_activity_nvo_egreso"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".NvoIngreso"
            android:label="@string/title_activity_nvo_ingreso"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>